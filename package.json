{
  "name": "mind-voice-box",
  "version": "1.0.0",
  "description": "중고등학생을 위한 온라인 건의함 서비스",
  "main": "server.js",
  "scripts": {
    "start": "npm --prefix ./client starts",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "concurrently": "^8.0.1"
  }
} 
